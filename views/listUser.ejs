<%-include('header')%>
<table class="table">
    <thead>
      <tr>
        <th scope="col">userId</th>
        <th scope="col">Name</th>
        <th scope="col">Mobile No</th>
        <th scope="col">Email Address</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <% mani.forEach(function(obj){%>
      <tr>
        <th scope="row">1</th>
        <td><%=obj.userName%></td>
        <td><%=obj.userphoneNumber%></td>
        <td><%=obj.userEmail%></td>
        
        <!-- <td><a href="/edit/<%=obj._id%>"><i class="far fa-edit"></i></a></td> -->
        <td><a href="/delete/<%=obj._id%>"><i class="far fa-trash-alt"></i></a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
<%-include('footer')%>
